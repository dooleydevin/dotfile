<main>
  <h1>
    {{ $on := .Data.hash }}
    {{ $against := .Data.against }}
    {{ if not .Owned }}
    <a href="/{{ .Vars.username }}">{{ .Vars.username }}</a> / 
    {{ end }}
    <a href="/{{ .Vars.username }}/{{ .Vars.alias }}">{{ .Vars.alias }}</a> @
    <a href="/{{ .Vars.username }}/{{ .Vars.alias }}/{{ $on }}">{{ shortenHash $on }}</a> against
    <a href="/{{ .Vars.username }}/{{ .Vars.alias }}/{{ $against }}">{{ shortenHash $against }}</a>
  </h1>
  {{ template "diff" . }}
</main>
