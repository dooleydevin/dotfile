<main>
  <h1>{{ .Title }}</h1>
  <form method="post">
    <label for="password">New Password</label>
    <input id="password" name="password" type="password" required>
    <label for="confirm">Confirm</label>
    <input id="confirm" name="confirm" type="password" required>
    <button>Submit</button>
  </form>
</main>
